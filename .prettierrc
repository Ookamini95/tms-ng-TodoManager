{
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "printWidth": 80,
    "tabWidth": 4,
    "useTabs": false,
    "endOfLine": "lf"
}